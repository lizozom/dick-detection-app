{
    "compilerOptions": {
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
    },

    "include": ["src/**/*", "globals.d.ts"],
    "exclude": ["node_modules", "dist", "cpp"]
  }